</head>
<style>
    .margin0 {
        margin-left: 0px !important;
    }
    
    .icon-hidden {
        display: none;
    }

    .hidden-sidebar{
        display: none;
    }
    .icon-2{
        display:none;
    }

</style>
<body class="hold-transition">
    <div id="preloader">
        <div id="preloader-inner"></div>
    </div>
    <div class="wrapper" id="nav_vue">
        <partial name="_AdminlteMainHeader" model="_GetSharedUIData.LoginViewModel" />
        <partial name="_AdminlteMainSidebar" model="_GetSharedUIData" />

        <div class="content-wrapper" id="content-nav">
            <main role="main" class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </main>
        </div>

        <partial name="~/Views/UIComponents/_SmallModal.cshtml" />
        <partial name="~/Views/UIComponents/_MediumModal.cshtml" />
        <partial name="~/Views/UIComponents/_BigModal.cshtml" />
        <partial name="~/Views/UIComponents/_ExtraBigModal.cshtml" />
        <partial name="~/Views/UIComponents/_ImageViewModal.cshtml" />

        <partial name="_AdminlteFooter" />
    </div>



    <script src="~/AdminLTE/plugins/daterangepicker/daterangepicker.js"></script>
    <script src="~/AdminLTE/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <script src="~/AdminLTE/dist/js/adminlte.js"></script>

    <script src="~/lib/sweetalert.min.js"></script>
    <script src="~/lib/sweetalert2/sweetalert2.js"></script>
    <script src="~/lib/toastr/toastr.min.js"></script>
    <script src="~/js/site.js"></script>





    <script src="~/DataTables/datatables.js"></script>
    <script src="~/AdminLTE/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/DataTables/buttons.dataTables.js"></script>

    <script src="~/DataTables/Export/pdfmake-0.1.36/pdfmake.js"></script>
    <script src="~/DataTables/Export/pdfmake-0.1.36/vfs_fonts.js"></script>
    <script src="~/DataTables/Export/Buttons-1.6.5/js/buttons.html5.min.js"></script>
    <script src="~/DataTables/Export/Buttons-1.6.5/js/buttons.print.min.js"></script>

    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <link href="~/AdminLTE/plugins/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <script src="~/AdminLTE/plugins/sweetalert2/sweetalert2.min.js"></script>

    <script src="~/lib/JsBarcode.all.min.js"></script>
    <script src="~/AdminLTE/plugins/select2/js/select2.js"></script>
    <script src="~/lib/loadingoverlay.js"></script>
    <script src="~/lib/ua-parser.min.js"></script>
    <script src="~/lib/jquery-treeview/jstree.js"></script>
    <script src="~/lib/jquery.preloader.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />
    <script type="text/javascript">
        var showMenu = false;
        var showButton = false;

        function toggleMenu() {
            showMenu = !showMenu;
            showButton = !showButton;
            var mainSidebar = document.getElementById('main-sidebar');
            var iconClick = document.getElementById('icon-1');
            var iconClick2 = document.getElementById('icon-2');
            if (showButton) {
                iconClick.classList.add('icon-hidden');
                iconClick2.classList.remove('icon-2');
            }else{
                iconClick.classList.remove('icon-hidden');
                iconClick2.classList.add('icon-2');
            }
            if (showMenu) {
                mainSidebar.classList.add('hidden-sidebar');
            } else {
                mainSidebar.classList.remove('hidden-sidebar');
            }
            if (showMenu) {
                document.getElementById('nav-header').classList.add('margin0');
                document.getElementById('content-nav').classList.add('margin0');
            } else {
                document.getElementById('nav-header').classList.remove('margin0');
                document.getElementById('content-nav').classList.remove('margin0');
            }
        }
    </script>
</body>
</html>

